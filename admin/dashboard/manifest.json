  "related_applications": [],
  "prefer_related_applications": false,
  "features": [
    "Cross Platform",
    "Fast",
    "Simple"
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "file_handlers": [
    {
      "action": "/wp-admin/admin.php?page=ai-news-dashboard&handler=import",
      "accept": {
        "text/csv": [".csv"],
        "application/json": [".json"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+aanp",
      "url": "/wp-admin/admin.php?page=ai-news-dashboard&action=%s"
    }
  ],
  "share_target": {
    "action": "/wp-admin/admin.php?page=ai-news-dashboard&share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["image/*", "text/*", "application/pdf"]
        }
      ]
    }
  },
  "handle_links": "preferred",
  "capture_links": "existing-client-navigate"
}